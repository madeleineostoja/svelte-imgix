{
  "name": "svelte-imgix",
  "description": "Svelte action for responsive, lazily-loaded images with Imgix",
  "version": "2.2.4",
  "license": "MIT",
  "author": "Madeleine Ostoja",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "develop": "rollup -cw",
    "build": "rollup -c",
    "prepublishOnly": "yarn build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "svelte",
    "svelte-action",
    "imgix",
    "responsive-images",
    "lazyload",
    "intersection-observer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/peppercorntsudio/svelte-imgix"
  },
  "bugs": {
    "url": "https://github.com/peppercorntsudio/svelte-imgix/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "prettier": "^2.3.0",
    "rollup": "^2.48.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "query-string": "^7.0.0"
  }
}
